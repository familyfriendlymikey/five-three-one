{
  "name": "fto",
  "scripts": {
    "start": "imba run -SMH --baseurl . -w server.imba",
    "build": "rm -rf dist && imba build -SMH --baseurl . server.imba",
    "pages": "npx gh-pages --no-history --dotfiles --dist dist/public --branch deploy",
    "deploy": "npm run build && npm run pages"
  },
  "dependencies": {
    "express": "^4.18.1",
    "imba": "^2.0.0-alpha.215"
  },
  "devDependencies": {
    "@types/node": "^18.7.8",
    "gh-pages": "^4.0.0"
  },
  "description": "A simple five-three-one calculator.",
  "version": "1.0.0",
  "homepage": "https://fto.pages.dev/",
  "author": "Mikey Oz (https://github.com/familyfriendlymikey)"
}
